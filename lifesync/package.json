{
  "name": "lifesync",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:coverage": "vitest --coverage",
    "test:project-tracking": "vitest --config ./vitest.config.projecttracking.ts src/pages/__tests__/ProjectTracking*.test.tsx",
    "test:project-tracking:watch": "vitest --config ./vitest.config.projecttracking.ts src/pages/__tests__/ProjectTracking*.test.tsx --watch",
    "test:project-tracking:coverage": "vitest --config ./vitest.config.projecttracking.ts src/pages/__tests__/ProjectTracking*.test.tsx --coverage",
    "test:project-tracking:ui": "vitest --config ./vitest.config.projecttracking.ts src/pages/__tests__/ProjectTracking*.test.tsx --ui",
    "version": "node scripts/version.js",
    "version:bump": "node scripts/version.js bump",
    "version:bump:major": "node scripts/version.js bump major",
    "version:bump:minor": "node scripts/version.js bump minor",
    "version:bump:patch": "node scripts/version.js bump patch",
    "version:generate": "node scripts/version.js generate",
    "version:history": "node scripts/version.js history",
    "release:prepare": "npm run lint && npm run test && npm run version:generate && npm run build"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@headlessui/react": "^2.2.7",
    "@supabase/supabase-js": "^2.57.4",
    "@tiptap/pm": "^3.4.0",
    "@tiptap/react": "^3.4.0",
    "@tiptap/starter-kit": "^3.4.0",
    "@types/leaflet": "^1.9.20",
    "@types/pg": "^8.15.5",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "express": "^5.1.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.542.0",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-leaflet": "^5.0.0",
    "recharts": "^3.1.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.10",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.55.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  }
}
